<launch4jConfig>
  <headerType>console</headerType>
  <dontWrapJar>true</dontWrapJar>
  <jar>launch4j.jar</jar>
  <outfile>../launch4jc.exe</outfile>
  <errTitle>launch4j</errTitle>
  <stayAlive>false</stayAlive>
  <jre>
  	<path>%JAVA_HOME%;%PATH%</path>
    <minVersion>1.8.0</minVersion>
  </jre>
  <versionInfo>
    <fileDescription>Cross-platform Java application wrapper</fileDescription>
    <copyright>Copyright (C) 2004, 2022 Grzegorz Kowal</copyright>
    <productName>launch4j</productName>
    <companyName>http://launch4j.sourceforge.net/</companyName>
    <internalName>Console launcher</internalName>
    <originalFilename>launch4jc.exe</originalFilename>
  </versionInfo>
</launch4jConfig>